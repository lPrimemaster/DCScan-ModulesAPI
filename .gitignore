# Leave visual studio settings out of the way
.vscode/
.vs/
*.vcxproj
*.vcxproj.filters

# Leave visual studio code out as well
.vscode/

# clang cache
.cache/

# Ignore cmake generator files created at build time
*.cmake
*.stamp
*.stamp.depend

# Track custom made cmake files
!cmake/modules/*.cmake

# Do not track builds or installations
[Bb]uild/
[Ii]nstall/

# No need to track the config files
/config/registry.*
/config/rpc.h

# Don't track python cache
__pycache__/

# Don't track cmake compile commands
compile_commands.json

# And settings
CMakeSettings.json
